@model IEnumerable<trialofAD.Models.Group>
@{
    ViewBag.Title = "GetAllGroups";
}
<br />

<table class="table table-striped table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GroupName)
        </th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.GroupName)
    </td>

    <!--<td><a asp-page="./AssignUser/" asp-route-GroupId="@item.Id">Assign User</a> </td>-->
    <td><a asp-page="./DeleteUser/" asp-route-GroupId="@item.Id">Delete User</a> </td>

</tr>
    }
</table>

<p>
    <a class="btn btn-primary" asp-page="./Create">
        Add New
    </a>
</p>

